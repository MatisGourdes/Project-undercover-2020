      <Button fx:id="addPlayerBtn" layoutX="491.0" layoutY="61.0" mnemonicParsing="false" onAction="#addPlayer" text="Add" />
      <TextField fx:id="eingabeName" layoutX="342.0" layoutY="61.0" promptText="SpielerName" />
      <Label fx:id="spielerNrLabel" layoutX="399.0" layoutY="37.0" prefHeight="17.0" prefWidth="83.0" text="Spieler Nr" />
      <TextField fx:id="eingabeAnzahlSpieler" layoutX="75.0" layoutY="61.0" prefHeight="25.0" prefWidth="158.0" promptText="Zahl zw 4 und 8 eingeben" />
      <Button fx:id="speichern" layoutX="128.0" layoutY="86.0" mnemonicParsing="false" onAction="#save" text="speichern" />
   </children>
</AnchorPane>
